package Exam.Exercice;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.PreparedStatement;
import java.sql.ResultSet;
import java.sql.SQLException;


public class PostgreSQLConnector throws SQLException{
	String psqlUrl = "jdbc:postgresql://localhost:5432/your_postgresql_database";
	String psqlUser = "your_postgresql_username";
	String psqlPassword = "your_postgresql_password";

	Connection psqlConn = DriverManager.getConnection(psqlUrl, psqlUser, psqlPassword);
}
